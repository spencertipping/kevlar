<!doctype html>
<html>
  <head>
  <title>Kevlar RPC test</title>
  <script src='//ajax.googleapis.com/ajax/libs/jquery/1.6.1/jquery.js'></script>

  <script src='//caterwauljs.org/build/caterwaul.js'></script>
  <script src='//caterwauljs.org/build/extensions/std.pre.js'></script>
  <script src='//caterwauljs.org/build/extensions/ui.pre.js'></script>
  <script src='../kevlar.client.jquery.js'></script>

  <script>
    $(caterwaul.js_ui(caterwaul.js_all())(function () {
      var rpc = kevlar().auto(given.nothing in $('body').append('kevlar auto-configuration complete'));

      $('body').append(lhs, rhs, rpc_button, output),
      where [lhs        = jquery in input.lhs,
             rhs        = jquery in input.rhs,
             output     = jquery in textarea.output,

             send_rpc_request() = rpc.add_two_things(lhs.val(), rhs.val(), given.reply in output.val(reply)),
             rpc_button         = jquery in button('Add these things') /click(send_rpc_request)];
    }));
  </script>
  </head>
  <body></body>
</html>

